<ul class="table-row _item-row">
	<li class="table__cell table-select-row">
		<obj name="checkbox" type="obj_checkbox" tabindex="true"></obj>
	</li>
	<li class="table__cell oauth-client_id no-wrap" id="{anchor oauth_}{item::clientid}">
		{item::clientid}
	</li>
	<li class="table__cell oauth-name no-wrap">
		{item::name}
	</li>
	<li class="table__cell oauth-description">
		{item::description}
	</li>
	<li class="table__cell oauth-redirect_uri">
		{item::redirecturi}
	</li>
	<li class="table__cell oauth-auth_type text-center">
		{item::auth_type}
	</li>
</ul>
