<div class="palette" id="{anchor palette}"><div id="{anchor cross}"></div></div>
<div class="light" id="{anchor light}"><div class="arrow" id="{anchor arrow}"></div></div>
<div class="color" id="{anchor color}"></div>

<table class="frmtbl">
	<tr>
		<th class="th">R</th><td><obj name="r" type="obj_input_number" css="obj_input_small"><value>136</value></obj></td>
	</tr>
	<tr>
		<th class="th">G</th><td><obj name="g" type="obj_input_number" css="obj_input_small"><value>136</value></obj></td>
	</tr>
	<tr>
		<th class="th">B</th><td><obj name="b" type="obj_input_number" css="obj_input_small"><value>136</value></obj></td>
	</tr>
	<tr>
		<td colspan="2" class="space"></td>
	</tr>
	<tr>
		<th class="th">RGB</th><td><obj name="rgb" type="obj_input" css="obj_input_small"><value>#888888</value></obj></td>
	</tr>
</table>

<div class="standard" id="{anchor standard}">
	<div rel="#C00000" style="background: #C00000"></div>
	<div rel="#FF0000" style="background: #FF0000"></div>
	<div rel="#FFC000" style="background: #FFC000"></div>
	<div rel="#FFFF00" style="background: #FFFF00"></div>
	<div rel="#92D050" style="background: #92D050"></div>
	<div rel="#00B050" style="background: #00B050"></div>
	<div rel="#00B0F0" style="background: #00B0F0"></div>
	<div rel="#0070C0" style="background: #0070C0"></div>
	<div rel="#002060" style="background: #002060"></div>
	<div rel="#7030A0" style="background: #7030A0"></div>
</div>