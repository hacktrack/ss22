<div class="popupmaindialog">
	<table class="frmtbl frmtbl100">
		<tr>
			<th colspan="2">{CONFERENCE::DETAILS}</th>
		</tr>
		<tr>
			<td class="th">{CONFERENCE::SHORTURL}</td>
			<td><obj name="url" type="obj_input" css="obj_input_100"><readonly>1</readonly></obj></td>
		</tr>
		<tr>
			<td class="th">{CONFERENCE::SHORTID}</td>
			<td><obj name="id" type="obj_input" css="obj_input_100"><readonly>1</readonly></obj></td>
