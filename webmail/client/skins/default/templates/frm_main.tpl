<div id="top_banner">
	<img class="logo" src="{_skin}/images/banner_logo.gif" />
	<div class="top-banner" id="banner-container"></div>
</div>
<div class="menu">
	<div class="menu1" id="{anchor menu1}"></div>
	<div class="menu2" id="{anchor menu2}"></div>
	<div class="menu3" id="{anchor menu3}"></div>
</div>
<div class="toolbar">
	<div class="toolbar1" id="{anchor toolbar1}"></div>
</div>

<div id="{anchor menu4}" class="filter">
	<obj name="filter" type="obj_view_filter"></obj>
</div>

<div class="box">
	<div class="container" id="{anchor container}">
		<div class="main">
			<div id="{anchor main}" class="maxbox relative"></div>
			<div id="{anchor dock}" class="dock"><obj name="dock" type="obj_vdock"><init>1</init></obj></div>
		</div>

		<div id="{anchor left}" class="tree small"></div>
		<div id="{anchor im}" class="im small"></div>
		<div id="{anchor slider}" class="slider y"></div>
		<div id="{anchor slider2}" class="slider y slider2"></div>
	</div>
</div>
