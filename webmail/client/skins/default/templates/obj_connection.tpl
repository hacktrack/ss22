<table class="frmtbl nospace">
	<tr>
		<td width="100%" id="{anchor errors}" class="errormessages"></td>
		<td class="buttons">
			<div id="{anchor retry}"><obj name="btn_re" type="obj_button" css="retry big noborder simple color1"><value>FORM_BUTTONS::RETRY</value></obj></div>
			<div id="{anchor accept}"><obj name="btn_ok" type="obj_button" css="ok big noborder simple color1"><value>FORM_BUTTONS::OK</value></obj></div>
			<div id="{anchor countdown}" class="countdown"></div>
		</td>
		<td id="{anchor cancel}"><obj name="btn_cancel" type="obj_button" css="cancel big noborder simple color2"><value>FORM_BUTTONS::CANCEL</value></obj></td>
	</tr>
</table>