<div id="{anchor container}" class="container" unselectable="on">
	<div class="header" id="{anchor top}">
		<div id="{anchor rem}" class="button rem" unselectable="on"></div>
		<div id="{anchor max}" class="button max" unselectable="on"></div>
		<div id="{anchor min}" class="button min" unselectable="on"></div>
		<div id="{anchor ico}" class="obj_popupico" unselectable="on"></div>
		<div id="{anchor title}" class="title" unselectable="on" dir="auto"><span id="{anchor title_text}"></span></div>
	</div>
		
	<div class="container2" unselectable="on">
		<div class="obj_popupmain" id="{anchor box}">
			<div class="maxbox">
				<div class="obj_popupmainbody">
					<div class="obj_popupmainbody_main" id="{anchor main}"></div>
				</div>
				<div class="obj_popupmainheader" id="{anchor header}"></div>
				<div class="obj_popupfoot" id="{anchor footer}"></div>
			</div>
		</div>
	</div>
</div>