<div>
	<div class="image" id="{anchor image}"></div>
	<div class="info">
		<h2>{title}</h2>
		<h3>{url}</h3>
		<p>{desc}</p>
	</div>
	{optional buttons}
	<div class="btn"><span id="{anchor prev}"></span><span id="{anchor next}"></span><b id="{anchor tbn}"></b></div>
	{/optional}
	<div class="close" id="{anchor close}"></div>
	<div class="enter">{CHAT::ENTER_TO_SEND}</div>
</div>